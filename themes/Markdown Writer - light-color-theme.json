{
    "name": "Markdown Writer - light",
    "type": "light",
    "colors": {
        "activityBar.background": "#f4f6f6",
        "activityBar.foreground": "#abc6c9",
        "activityBarBadge.background": "#dce6e7",
        "activityBarBadge.foreground": "#49868e",
        "dropdown.background": "#f4f6f6",
        "dropdown.border": "#dce6e7",
        "dropdown.foreground": "#005661",
        "editor.background": "#f4f6f6",
        "editor.findMatchBackground": "#dcecf5",
        "editor.findMatchHighlightBackground": "#dcecf5",
        "editor.findRangeHighlightBackground": "#dcecf5",
        "editor.foreground": "#005661",
        "editor.hoverHighlightBackground": "#e8f1f6",
        "editor.inactiveSelectionBackground": "#edf1f2",
        "editor.lineHighlightBackground": "#eff3f3",
        "editor.rangeHighlightBackground": "#e8f1f6",
        "editor.selectionBackground": "#e8f1f6",
        "editor.wordHighlightBackground": "#e8f1f6",
        "editor.wordHighlightStrongBackground": "#0094f0",
        "editorBracketMatch.background": "#f4f6f6",
        "editorBracketMatch.border": "#dce6e7",
        "editorCursor.foreground": "#0094f0",
        "editorGroup.border": "#f2f4f5",
        "editorGroupHeader.tabsBackground": "#f4f6f6",
        "editorGutter.addedBackground": "#e8eeef",
        "editorGutter.deletedBackground": "#e8eeef",
        "editorGutter.modifiedBackground": "#e8eeef",
        "editorIndentGuide.background": "#f4f6f6",
        "editorLineNumber.foreground": "#dcecf5",
        "editorRuler.foreground": "#dce6e7",
        "editorWhitespace.foreground": "#dcecf5",
        "editorWidget.background": "#f4f6f6",
        "editorWidget.border": "#c3d6d8",
        "editorWidget.foreground": "#005661",
        "editorWidget.resizeBorder": "#0094f0",
        "input.background": "#f4f6f6",
        "input.border": "#dce6e7",
        "input.foreground": "#005661",
        "input.placeholderForeground": "#31767f",
        "inputOption.activeBorder": "#0094f0",
        "list.activeSelectionBackground": "#e8f1f6",
        "list.activeSelectionForeground": "#49868e",
        "list.focusBackground": "#dcecf5",
        "list.hoverBackground": "#edf1f2",
        "list.inactiveSelectionBackground": "#e8eeef",
        "panel.border": "#dce6e7",
        "peekViewEditor.matchHighlightBackground": "#dce6e7",
        "peekViewResult.matchHighlightBackground": "#e8f1f6",
        "quickInput.background": "#f4f6f6",
        "quickInput.foreground": "#005661",
        "quickInputTitle.background": "#e8eeef",
        "scrollbarSlider.activeBackground": "#00566130",
        "scrollbarSlider.background": "#00566110",
        "scrollbarSlider.hoverBackground": "#00566120",
        "sideBar.background": "#f4f6f6",
        "sideBar.border": "#dce6e7",
        "sideBar.foreground": "#005661",
        "sideBarSectionHeader.background": "#f4f6f6",
        "sideBarSectionHeader.foreground": "#49868e",
        "statusBar.background": "#f4f6f6",
        "statusBar.foreground": "#49868e",
        "tab.activeBackground": "#f4f6f6",
        "tab.activeBorder": "#f4f6f6",
        "tab.activeForeground": "#49868e",
        "tab.border": "#dce6e7",
        "tab.inactiveBackground": "#f4f6f6",
        "tab.inactiveForeground": "#abc6c9",
        "titleBar.activeBackground": "#f4f6f6",
        "titleBar.activeForeground": "#49868e",
        "titleBar.inactiveBackground": "#f4f6f6",
        "titleBar.inactiveForeground": "#abc6c9",
        "focusBorder": "#0094f0",
        "foreground": "#49868e"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#abc6c9"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "foreground": "#abc6c9"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#62969d"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#62969d"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#c3d6d8"
            }
        },
        {
            "name": "HTML: Doctype Declaration",
            "scope": [
                "meta.tag.sgml.doctype",
                "meta.tag.sgml.doctype string",
                "meta.tag.sgml.doctype entity.name.tag",
                "meta.tag.sgml punctuation.definition.tag.html"
            ],
            "settings": {
                "foreground": "#abc6c9"
            }
        },
        {
            "name": "HTML: Tags",
            "scope": [
                "meta.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#62bbf2"
            }
        },
        {
            "name": "HTML: Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "HTML: Attribute Names",
            "scope": [
                "meta.tag entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#62bbf2"
            }
        },
        {
            "name": "HTML: Entities",
            "scope": [
                "constant.character.entity",
                "punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "CSS: Selectors",
            "scope": [
                "meta.selector",
                "meta.selector entity",
                "meta.selector entity punctuation",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "CSS: Property Names",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "CSS: Property Values",
            "scope": [
                "meta.property-value",
                "meta.property-value constant.other",
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "CSS: Important Keyword",
            "scope": "keyword.other.important",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Markup: Deletion",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup: Error",
            "scope": "markup.error",
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Markup: Insertion",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Markup: Link",
            "scope": "meta.link",
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Markup: Output",
            "scope": [
                "markup.output",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Markup: Prompt",
            "scope": "markup.prompt",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Markup: Strong",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Traceback",
            "scope": "markup.traceback",
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "Markup: Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#62969d",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Markup Styling",
            "scope": [
                "markup.bold",
                "markup.italic"
            ],
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#005661"
            }
        },
        {
            "name": "Extra: Diff Range",
            "scope": [
                "meta.diff.range",
                "meta.diff.index",
                "meta.separator"
            ],
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Extra: Diff From",
            "scope": "meta.diff.header.from-file",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Extra: Diff To",
            "scope": "meta.diff.header.to-file",
            "settings": {
                "foreground": "#005661"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "foreground": "#005661",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Heading punctuation",
            "scope": [
                "punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#abc6c9",
                "fontStyle": ""
            }
        },
        {
            "name": "Link Reference Def",
            "scope": [
                "string.other.link.description.title.markdown"
            ],
            "settings": {
                "foreground": "#49868e"
            }
        },
        {
            "name": "reference link markdown",
            "scope": [
                "constant.other.reference.link.markdown",
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#c3d6d8",
                "fontStyle": ""
            }
        },
        {
            "name": "wiki-link punctuation",
            "scope": [
                "punctuation.definition.wiki-link",
                "punctuation.definition.string.begin.markdown",
                "punctuation.definition.string.end.markdown",
                "punctuation.definition.metadata.markdown",
                "punctuation.definition.constant.markdown"
            ],
            "settings": {
                "foreground": "#dcecf5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "reference link markdown",
            "scope": [
                "string.other.link.title.markdown",
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#0094f0",
                "fontStyle": ""
            }
        },
        {
            "name": "wiki-link content",
            "scope": [
                "support.function.text.markdown.writer.wiki-link.title"
            ],
            "settings": {
                "foreground": "#0094f0"
            }
        },
        {
            "name": "List bullet",
            "scope": [
                "punctuation.definition.list.begin.markdown"
            ],
            "settings": {
                "foreground": "#005661"
            }
        }
    ]
}
